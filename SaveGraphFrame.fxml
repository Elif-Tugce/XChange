<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #AFD3E2;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SaveGraphFrameController">
   <children>
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="600.0" style="-fx-background-color: #F7FAFC;" text="Save A New Graph">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Text layoutX="226.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name of the graph">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="graphNameTextField" layoutX="13.0" layoutY="75.0" prefHeight="25.0" prefWidth="571.0" />
      <Text layoutX="202.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description of the graph" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="graphDescriptionTextField" layoutX="13.0" layoutY="155.0" prefHeight="25.0" prefWidth="571.0" />
      <Text layoutX="234.0" layoutY="229.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Importance (1-5)">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="graphImportanceTextField" layoutX="13.0" layoutY="236.0" prefHeight="25.0" prefWidth="571.0" />
      <Button layoutX="246.0" layoutY="330.0" mnemonicParsing="false" onMouseClicked="#saveGraphButtonAction" prefHeight="25.0" prefWidth="105.0" text="Save" />
      <Label fx:id="errorLabel" layoutX="168.0" layoutY="292.0" textFill="RED" />
   </children>
</AnchorPane>
